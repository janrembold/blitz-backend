{
  "name": "blitz-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node dist/index.js",
    "dev": "cross-env DEV_CONNECTION=postgres://postgres:supersecret@localhost:5432/blitz ts-node src/index.js",
    "migration.create": "cross-env DEV_CONNECTION=postgres://postgres:supersecret@localhost:5432/blitz node src/migrate/migrateCLI.js create --name",
    "build": "tsc",
    "dev.app": "cross-env PORT=5000 DEV_CONNECTION=postgres://postgres:supersecret@localhost:5432/blitz ts-node src/migrate/migrate.js",
    "dev.db": "docker-compose -f docker-compose.yml up"
  },
  "dependencies": {
    "@graphql-tools/schema": "8.3.1",
    "apollo-server-core": "3.5.0",
    "apollo-server-express": "3.5.0",
    "aws-sdk": "2.1009.0",
    "cors": "2.8.5",
    "date-fns": "2.27.0",
    "express": "4.17.1",
    "express-jwt": "6.1.0",
    "graphql": "16.0.1",
    "graphql-subscriptions": "2.0.0",
    "jsonwebtoken": "8.5.1",
    "knex": "0.95.14",
    "pg": "8.7.1",
    "pg-boss": "6.2.2",
    "pg-format": "1.0.4",
    "slonik": "24.1.2",
    "subscriptions-transport-ws": "0.11.0"
  },
  "devDependencies": {
    "@slonik/migrator": "0.8.5",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-jwt": "6.0.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/node": "16.11.1",
    "@types/pg": "8.6.1",
    "@types/pg-format": "1.0.2",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "cross-env": "7.0.3",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "ts-node": "10.3.0",
    "typescript": "4.4.4"
  }
}
